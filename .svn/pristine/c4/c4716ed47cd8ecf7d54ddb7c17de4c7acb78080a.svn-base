    <script type="text/javascript">

  $(function(){
   var ww;
   var hh;
   var i=0; 
   var nav=$('.JQ-slide-nav')
   var con=$('.img-list ul')
     var img=new Image();
    img.src="img/newImg/pic2.jpg";
    img.onload = function(){
       hh=$('.img_scroll li').height();
       $('.ck_noname').css({'top':hh});
       $('.ck_focusBox_kongge').css({'height':hh});
       $('.cityBox,.popBox').css({'top':hh+54});
       
       
    }

     var ww=$(window).width();
    $('.img_scroll').css({'width':ww});
    $('.img-list').css({'width':ww});
    $('.img-list li').css({'width':ww});
var MyMar = setInterval(Marquee, 8000);
var l=$('.img-list li').length;
function Marquee(){
 i++;
  if(i==l){
    i=0;
    }
  nav.find('li').removeClass('on');
  nav.find("li:eq("+i+")").addClass('on');
   con.animate({'margin-left':-ww},function()
    {
    con.find('li').eq(0).appendTo(con);
    con.css({'margin-left':0});
    });
}
con.touchwipe( {
wipeRight:function(){
 i--;
  if(i==-1){
    i=l-1;
    }
     nav.find('li').removeClass('on');
    nav.find("li:eq("+i+")").addClass('on');
  con.find('li:last').prependTo(con);
    con.css({'margin-left':-ww});
    con.animate({'margin-left':0});
},
wipeLeft:function(){
  i++;
  if(i==l){
    i=0;
    }
  nav.find('li').removeClass('on');
  nav.find("li:eq("+i+")").addClass('on');
   con.animate({'margin-left':-ww},function()
    {
    con.find('li').eq(0).appendTo(con);
    con.css({'margin-left':0});
    });
},
min_move_x: 20,
min_move_y: 20,
preventDefaultEvents: true
});
    // DY_scroll('.img_scroll','.prev','.next','.JQ-slide-nav','.img-list',5,true);
       
    })

</script>
